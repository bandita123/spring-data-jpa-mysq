pipeline {
    agent any

    stages {
        stage('Init') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'mvn clean package'
            }
            post{
                success{
                    archiveArtifacts artifacts:'**/target/*.war'
                }

            }

        }
        stage('Deployed in staging') {
            steps {
                build job:'deployed-in-staging'
            }
        }
    }
}